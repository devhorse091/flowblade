{
  "name": "@flowblade/source-kysely",
  "description": "Kysely datasource",
  "version": "0.12.0",
  "license": "MIT",
  "author": {
    "name": "Vanvelthem SÃ©bastien",
    "url": "https://github.com/belgattitude"
  },
  "homepage": "https://belgattitude.github.io/flowblade",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/belgattitude/flowblade.git",
    "directory": "packages/source-kysely"
  },
  "keywords": [],
  "sideEffects": false,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "?build-release": "When https://github.com/atlassian/changesets/issues/432 has a solution we can remove this trick",
    "build": "run clean && yarn run tsup",
    "build-release": "run build",
    "bench": "vitest bench --run",
    "bench-codspeed": "cross-env CODSPEED=1 vitest bench --run",
    "bench-watch": "vitest bench",
    "docgen": "run-s docgen-typedoc",
    "docgen-typedoc": "rimraf ./docs/api && typedoc --plugin typedoc-plugin-markdown --out ./docs/api",
    "check-dist": "run-s check-dist-esm check-dist-cjs",
    "check-dist-cjs": "es-check --not './dist/*.map.js' -v es2022 './dist/**/*.cjs'",
    "check-dist-esm": "es-check --not './dist/*.map.js' -v es2022 --module './dist/**/*.mjs'",
    "check-pub": "attw --pack && publint",
    "check-size": "size-limit",
    "clean": "rimraf --glob ./dist ./build ./coverage ./_release './tsconfig*.tsbuildinfo'",
    "dev": "tsup --watch",
    "fix-staged": "lint-staged --allow-empty",
    "kysely": "kysely",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx,.mjs,.cjs,.mts,.cts --cache --cache-location ../../.cache/eslint/dsn-parser.eslintcache",
    "e2e-codegen": "tsx scripts/run-e2e-codegen.ts",
    "e2e-recreate-db": "tsx scripts/run-e2e-recreate-db.ts",
    "e2e-docker-up": "docker compose -f ./e2e/docker/sql-edge/compose.yml up",
    "e2e-docker-up-d": "docker compose -f ./e2e/docker/sql-edge/compose.yml up -d --wait",
    "e2e-docker-down": "docker compose -f ./e2e/docker/sql-edge/compose.yml down --volumes",
    "test": "vitest run",
    "test-unit": "vitest run",
    "test-e2e": "vitest --config=vitest.e2e.config.ts run",
    "test-unit-coverage": "vitest run --coverage",
    "test-unit-edge": "vitest run --environment edge-runtime",
    "test-unit-watch": "vitest --ui --api 4515",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "typecheck-build": "tsc --project tsconfig.build.json --noEmit",
    "ci-coverage-upload": "../../.github/scripts/download/codecov -F flowblade-source-kysely --dir ./coverage"
  },
  "dependencies": {
    "@httpx/assert": "^0.15.1",
    "@httpx/dsn-parser": "^1.8.5",
    "@httpx/plain-object": "^2.0.3",
    "type-fest": "^4.31.0",
    "valibot": "^1.0.0-beta.9"
  },
  "peerDependencies": {
    "@flowblade/core": "workspace:^",
    "kysely": "^0.27.5",
    "tarn": "^3.0.2",
    "tedious": "^19.0.0"
  },
  "peerDependenciesMeta": {
    "tarn": {
      "optional": true
    },
    "tedious": {
      "optional": true
    }
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "0.17.2",
    "@belgattitude/eslint-config-bases": "6.17.0",
    "@codspeed/vitest-plugin": "4.0.0",
    "@edge-runtime/vm": "5.0.0",
    "@faker-js/faker": "9.3.0",
    "@prisma/client": "6.1.0",
    "@size-limit/file": "11.1.6",
    "@size-limit/webpack": "11.1.6",
    "@vitest/coverage-istanbul": "2.1.8",
    "@vitest/ui": "2.1.8",
    "browserslist": "4.24.3",
    "browserslist-to-esbuild": "2.1.1",
    "cross-env": "7.0.3",
    "es-check": "7.2.1",
    "es-toolkit": "1.31.0",
    "esbuild": "0.24.2",
    "eslint": "8.57.1",
    "execa": "9.5.2",
    "kysely": "0.27.5",
    "npm-run-all2": "7.0.2",
    "prettier": "3.4.2",
    "prisma": "6.1.0",
    "prisma-kysely": "1.8.0",
    "publint": "0.2.12",
    "rimraf": "6.0.1",
    "rollup": "4.29.1",
    "size-limit": "11.1.6",
    "tarn": "3.0.2",
    "tedious": "19.0.0",
    "tsup": "8.3.5",
    "tsx": "4.19.2",
    "typedoc": "0.27.6",
    "typedoc-plugin-markdown": "4.3.3",
    "typescript": "5.7.2",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "2.1.8",
    "webpack": "5.97.1"
  },
  "engines": {
    "node": ">=18.17.0"
  }
}
